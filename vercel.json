{
    "version": 2,
    "builds": [
      {
        "src": "api/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/advance_who_up_row",
        "dest": "/api/advance_who_up_row.js"
      },
      { 
        "src": "/index", 
        "dest": "/api/index.js" 
      },      
      { 
        "src": "/doctor", 
        "dest": "/public/doctor_view.html" 
      }
    ]
}